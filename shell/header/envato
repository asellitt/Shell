echo -e "$ERS"

echo -en "$EGRN"
echo -n '                   `:ohmNNNNNNNNNNNNNNNNNNNNNNNNNNds+-                          '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '                        `-/osyhdmmNNNNNNmddhso+:.                               '
