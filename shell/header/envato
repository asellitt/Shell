echo -en "$ELGRN"
echo -n '          /ooooooooo'
echo -en "$EGRN"
echo -n 'ymmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmms       '
echo -e "$ERS"

