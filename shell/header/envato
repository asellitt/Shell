echo -n '         `+oooooooos'
echo -en "$EGRN"
echo -n 'dmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmh`       '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '   hNNNNy'
