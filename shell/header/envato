echo -n 'NNNNNNNNNNNNNmmmmmmmmmmmmmmmmmmmmmmmmmmmmm-         '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '    -Nd`'
echo -en "$ELGRN"
echo -n '        .osssooooym'
echo -en "$EGRN"
echo -n 'NNNNNNNNNNNNNNNNNNNNNNmmmmmmmmmmmmmmmmmmmm-          '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '     -`'
echo -en "$ELGRN"
echo -n '        .osssssssd'
echo -en "$EGRN"
echo -n 'NNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNmmmmmmmmmmmd-           '
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '              .sssssssym'
echo -en "$EGRN"
echo -n 'NNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNmy`            '
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '             .sssssssh'
echo -en "$EGRN"
echo -n 'NNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNm+              '
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '            .sssssssd'
echo -en "$EGRN"
echo -n 'NNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNy.               '
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '            -ossssym'
echo -en "$EGRN"
echo -n 'NNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNy-                 '
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '              -+sh'
echo -en "$EGRN"
echo -n 'NNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNd+.                   '
echo -e "$ERS"

echo -en "$EGRN"
