echo -n 'mmmmmmmdddddddddy`'
echo -en "$ELGRN"
echo -n '           `/+++++++++++'
echo -en "$EGRN"
echo -n 'sdddddddddddddddddddddddddddddhhhh:   '
echo -e "$ERS"

echo -en "$EGRN"
echo -n 'mmmmmmmmmmmmdddh`'
echo -en "$ELGRN"
echo -n '           -+++++++++++'
echo -en "$EGRN"
echo -n 'ohdddddddddddddddddddddddddddddddddh`   '
echo -e "$ERS"

echo -en "$EGRN"
echo -n 'mmmmmmmmmmmmmmh.'
echo -en "$ELGRN"
echo -n '           :ooo+++++++'
echo -en "$EGRN"
echo -n 'oyddddddddddddddddddddddddddddddddddd+    '
echo -e "$ERS"

echo -en "$EGRN"
echo -n 'hmmmmmmmmmmmmd.'
echo -en "$ELGRN"
echo -n '          `/ooooooo+++'
echo -en "$EGRN"
echo -n 'sdmmmmdddddddddddddddddddddddddddddddd`    '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '+NNmmmmmmmmmm-'
echo -en "$ELGRN"
echo -n '          .+oooooooooo'
echo -en "$EGRN"
echo -n 'hmmmmmmmmmmmmmmmdddddddddddddddddddddd+     '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '.NNNNNNNmmmm:'
echo -en "$ELGRN"
echo -n '          -oooooooooo'
echo -en "$EGRN"
echo -n 'ymmmmmmmmmmmmmmmmmmmmmmmmmdddddddddddddh`     '
echo -e "$ERS"

echo -en "$EGRN"
echo -n ' yNNNNNNNNN/'
echo -en "$ELGRN"
