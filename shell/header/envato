ERS="\033[1;0m"          # echo-friendly reset
EGRN="\033[0;32m"        # echo-friendly foreground GREEN
ELGRN="\033[1;32m"     # echo-friendly foreground LIGHT GREEN

clear

echo -en "$ERS"

echo -en "$ELGRN"
echo -n '                                                                     `..-::::-. '
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '                                                                   .-::::::::::-'
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '                                                                `-::::::::::::::'
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '                                                               .::::::::::::::::'
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '                                                             .::::::::::::::::::'
echo -e "$ERS"

echo -en "$ELGRN"
echo -n '                                                           `:::::::::::::::::::'
echo -en "$EGRN"
echo -n '-'
echo -e "$ERS"

echo -en "$EGRN"
echo -n '                              `-:`'
echo -en "$ELGRN"
echo -n '                       `-:::::::::::::::::::'
echo -en "$EGRN"
echo -n '++'
echo -e "$ERS"

echo -en "$EGRN"
echo -n '                          .-/+++.'
echo -en "$ELGRN"
echo -n '                      `-::::::::::::::::::/'
echo -en "$EGRN"
echo -n '+os+'
echo -e "$ERS"

echo -en "$EGRN"
echo -n '                      `-/++++++-'
echo -en "$ELGRN"
echo -n '                      .://:::::::::::::::/'
echo -en "$EGRN"
echo -n 'ossss/'
echo -e "$ERS"

echo -en "$EGRN"
echo -n '                   ./+oooooooo:'
echo -en "$ELGRN"
echo -n '                     .:///////:::::::::/'
echo -en "$EGRN"
echo -n '+sssssss/'
echo -e "$ERS"

echo -en "$EGRN"
echo -n '                ./ooooooooooo:'
echo -en "$ELGRN"
echo -n '                    `:///////////:::::/'
echo -en "$EGRN"
echo -n 'osssssssss:'
echo -e "$ERS"

echo -en "$EGRN"
echo -n '             `:osssssooooooo/'
echo -en "$ELGRN"
echo -n '                    -/////////////////'
echo -en "$EGRN"
echo -n 'osssssssssss-'
echo -e "$ERS"

echo -en "$EGRN"
echo -n '           .+ssssssssssssso+'
echo -en "$ELGRN"
echo -n '                   .:///////////////'
echo -en "$EGRN"
echo -n '+oyyyyyyyssssss.'
echo -e "$ERS"

echo -en "$EGRN"
echo -n '         -oyysssssssssssss+'
echo -en "$ELGRN"
echo -n '                  `:///////////////'
echo -en "$EGRN"
echo -n '+syyyyyyyyyyyyyyy`'
echo -e "$ERS"

echo -en "$EGRN"
echo -n '       .oyyyyyyyysssssssso`'
echo -en "$ELGRN"
echo -n '                 -///////////////'
echo -en "$EGRN"
echo -n '+syyyyyyyyyyyyyyyyy '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '      /yyyyyyyyyyyyyyysss`'
echo -en "$ELGRN"
echo -n '                `://////////////'
echo -en "$EGRN"
echo -n '+syyyyyyyyyyyyyyyyyys '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '    `shhyyyyyyyyyyyyyyys.'
echo -en "$ELGRN"
echo -n '                -//////////////'
echo -en "$EGRN"
echo -n '+syyyyyyyyyyyyyyyyyyyy+ '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '   .yhhhhhhhhyyyyyyyyyy.'
echo -en "$ELGRN"
echo -n '               `/+////////////'
echo -en "$EGRN"
echo -n '+shhhhhhhyyyyyyyyyyyyyyy: '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '  .hhhhhhhhhhhhhhyyyyy-'
echo -en "$ELGRN"
echo -n '               -+++++////////'
echo -en "$EGRN"
echo -n '+shhhhhhhhhhhhhhhhhhyyyyyy. '
echo -e "$ERS"

echo -en "$EGRN"
echo -n ' `ydhhhhhhhhhhhhhhhhh:'
echo -en "$ELGRN"
echo -n '              `/+++++++++///'
echo -en "$EGRN"
echo -n '+shhhhhhhhhhhhhhhhhhhhhhhhhy  '
echo -e "$ERS"

echo -en "$EGRN"
echo -n ' oddddddhhhhhhhhhhhh/'
echo -en "$ELGRN"
echo -n '              -+++++++++++++'
echo -en "$EGRN"
echo -n 'shhhhhhhhhhhhhhhhhhhhhhhhhhho  '
echo -e "$ERS"

echo -en "$EGRN"
echo -n '.ddddddddddddhhhhhh+'
echo -en "$ELGRN"
echo -n '             `/++++++++++++'
echo -en "$EGRN"
echo -n 'ohhhhhhhhhhhhhhhhhhhhhhhhhhhhh:  '
echo -e "$ERS"

